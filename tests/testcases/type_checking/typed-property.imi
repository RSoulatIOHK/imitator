var x, y: clock;
i : rational;
j : int;
b : bool;
w : binary(4);

automaton P

    loc s0: invariant True
      when x<=1 & b = (True) & i = 1 & i = 1 do { w = 0b0001 } goto lend;

   accepting loc lend: invariant True

end


init := {

  discrete =
    loc[P] := s0,
    b := True,
    i := 1
  ;

  continuous =
    & x>=0
    & x=y
  ;
}
