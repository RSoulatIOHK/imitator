var 
    i
    	: int;
    j
        : rational;



(************************************************************)
 automaton pta
(************************************************************)
 
loc l1: invariant True 
	when 0 in [j, i] goto lend;
	
accepting loc lend: invariant True
 end (* pta *)
(************************************************************)


(************************************************************)
(* Initial state *)
(************************************************************)


init := {

    discrete = 
        loc[pta] := l1,
        i := 10,
        j := 15
    ;
    
}



