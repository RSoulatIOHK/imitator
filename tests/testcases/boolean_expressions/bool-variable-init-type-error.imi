var 
    x = 0,
        : clock;
    b
        : bool;


(************************************************************)
 automaton pta
(************************************************************)
 
loc l1: invariant True 
	when b goto lend;
	
accepting loc lend: invariant b
 end (* pta *)
(************************************************************)


(************************************************************)
(* Initial state *)
(************************************************************)


init := {

    discrete = 
        loc[pta] := l1,
        b := 2 + 2 
    ;
    continuous = 
        & x >= 0
    ;
    
}



(************************************************************)
(* The end *)
(************************************************************)
end
